automaton
{
	// Write
	cont {w}
	// Auth, LockOn, LockOff
	uncont {a, n, f}

	nodes
	{
		l0[initial];
		l1[accepting];
		l2[accepting];
		l3;
	}

	clocks {x}

	edges
	{
		l0 ->{a}{}{} l1;
		l0 ->{w}{}{} l3;
		l0 ->{n}{}{} l3;
		l0 ->{f}{}{} l3;
		l1 ->{n}{}{} l2;
		l1 ->{w}{}{x >= 2000} l1;
		l1 ->{f}{x}{} l1;
		l1 ->{a}{}{} l1;
		l1 ->{w}{}{x < 2000} l3;
		l2 ->{a}{}{} l2;
		l2 ->{n}{}{} l2;
		l2 ->{f}{x}{} l1;
		l2 ->{w}{}{} l3;
		l3 ->{w}{}{} l3;
		l3 ->{a}{}{} l3;
		l3 ->{n}{}{} l3;
		l3 ->{f}{}{} l3;
	}
}
	
